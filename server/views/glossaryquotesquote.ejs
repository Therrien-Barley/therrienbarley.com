    <!-- Le fav and touch icons -->
    <link rel="shortcut icon" href="/favicon.ico">
  </head>
<body data-spy="scroll" data-target=".subnav" data-offset="50" class="glossaryquotes">

	<div id="overlay">
		<div id="overlay-close" class="bold">Close</div>
		<div id="overlaybg"></div>
		<div id="overlay-quotes" class="row-fluid">
			<div class="post-left span6 offset1">
				<% for(var i in quotes){ %>
					<%- quotes[i] %>
				<% } %>
			</div>
		</div><!-- /#overlay-quotes -->
	</div><!-- /#overlay -->

	<div class="wrapper">
		<header id="header">
			<h6><a href="/alchemy">Alchemy 0.1</a></h6>
		</header>	
		<div id="reset"><a href="/fetch/tumblrposts">Reset!</a></div>	
		
		
		<div id="nav" class="row-fluid bold">
			<ul id="categories-nav" class="menu span3 offset4">
				<li class="menu-header">Categories</li>
				<li class="menu-option" value="categories-io"><a href="/alchemy/tumblrposts/categories/io">Percentage</a></li>
				<li class="menu-option" value="categories-posts"><a href="/alchemy/tumblrposts/categories/posts">Posts</a></li>
			</ul>

			<ul id="glossary-nav" class="menu span3 offset2 active-trail">
				<li class="menu-header">Glossary</li>
				<li class="menu-option" value="glossary-terms">
					<a href="/alchemy/tumblrposts/glossary/terms">Terms</a>
				</li>
				<li class="menu-option" value="glossary-definitions">
					<a href="/alchemy/tumblrposts/glossary/definitions">Definitions</a>
				</li>
				<li class="menu-option" value="glossary-headlines">
					<a href="/alchemy/tumblrposts/glossary/headlines">Headlines</a>
				</li>
				<li class="menu-option" value="glossary-quotes">
					<a href="/alchemy/tumblrposts/glossary/quotes" class="active">Quotes</a>
				</li>
				<li class="menu-option" value="glossary-collages">
					<a href="/alchemy/tumblrposts/glossary/collages">Collages</a>
				</li>
			</ul>
		</div><!-- /#nav -->

		

		<div id="main" class="row-fluid">
			<div class="span12 offset2 total-posts">
				<h6>Total Terms</h6>
				<div class="number bold"><%= terms.length %></div>
			</div>

			<div class="span12 offset2 terms">
				<% for(var tag in terms){ %>
					<div class="term"><a href="/alchemy/tumblrposts/glossary/quotes/quote/<%= terms[ tag ].tag.replace(/ /g,"-") %>" target="_blank"><%= terms[ tag ].tag %></a></div>
				<% } %>
			</div>
		</div><!-- /#main -->

		<footer id="footer" class="row-fluid">
			<div class="span6">
				Â© 2013 Therrien-Barley, all rights reserved<br>
				<a href="mailto:info@th-ey.co">info@th-ey.co</a>
			</div>
		</footer>

		
	</div><!-- /#wrapper -->



    <!-- Le javascript
    ================================================== -->
    <!-- Placed at the end of the document so the pages load faster -->
    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.9.1/jquery.min.js"></script>
	<script src="/javascripts/bootstrap.min.js"></script>
	<link rel="stylesheet" href="/stylesheets/elusive-webfont.css">
	<script src="/javascripts/select2.min.js"></script>
	<script src="/javascripts/alchemy.js"></script>

	
